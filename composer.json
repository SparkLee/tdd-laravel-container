{
  "name": "sparklee/tdd-laravel-container",
  "description": "用 TDD 重写 Laravel Container",
  "keywords": [
    "tdd",
    "container",
    "laravel"
  ],
  "license": "MIT",
  "homepage": "https://github.com/SparkLee/tdd-laravel-container",
  "support": {
    "issues": "https://github.com/SparkLee/tdd-laravel-container/issues",
    "source": "https://github.com/SparkLee/tdd-laravel-container/laravel/framework"
  },
  "authors": [
    {
      "name": "Spark Lee",
      "email": "liweijsj@163.com"
    }
  ],
  "require": {
    "php": "^7.3|^8.0",
    "ext-json": "*",
    "ext-mbstring": "*"
  },
  "require-dev": {
    "mockery/mockery": "^1.4.2",
    "phpunit/phpunit": "^8.5.8|^9.3.3"
  },
  "provide": {
    "psr/container-implementation": "1.0"
  },
  "autoload": {
    "psr-4": {
      "SparkLee\\": "src/SparkLee/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SparkLee\\Tests\\": "tests/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
